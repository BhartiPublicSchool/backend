      "type": "component",
      "repeatable": false,
      "component": "shared.footer"
    },
    "header_menu": {
      "type": "component",
      "repeatable": true,
      "component": "shared.header"
    },
    "quickMenu": {
      "type": "component",
      "repeatable": false,
      "component": "shared.quick-link"
    },
    "meta": {
      "type": "component",
      "repeatable": false,
      "component": "shared.page-meta-data"
    },
    "logo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "logo_mobile": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "Quicklink_dropdown": {
      "type": "component",
      "repeatable": false,
      "component": "shared.q-menu"
    }
  }
